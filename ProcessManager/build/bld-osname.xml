<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Runnable Jar for ProcMgr platform Jar-in-Jar Loader">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="OsNameArch.jar">
            <manifest>
                <attribute name="Main-Class" value="org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader"/>
                <attribute name="Rsrc-Main-Class" value="pmutils.OsNameArch"/>
                <attribute name="Class-Path" value="."/>
                <attribute name="Rsrc-Class-Path" value="./"/>
            </manifest>
            <zipfileset src="../lib/jar-in-jar-loader.zip"/>
            <fileset dir="." includes="pmutils/OsNameArch.class"/>
        </jar>
    </target>
</project>
